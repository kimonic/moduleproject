<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/button_corner" />
            <gradient android:startColor="#ffe06f6f" android:endColor="#ffe06f6f" android:centerColor="#ffe06f6f" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <corners android:radius="@dimen/button_corner" />
            <gradient android:startColor="#fffe7d7d" android:endColor="#fffe7d7d" android:centerColor="#fffe7d7d" />
        </shape>
    </item>
</selector>