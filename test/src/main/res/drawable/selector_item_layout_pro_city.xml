<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <stroke android:width="1.0dip" android:color="@color/buttonBluePressNormal" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="1.0dip" android:color="@color/buttonBluePress" />
        </shape>
    </item>
</selector>