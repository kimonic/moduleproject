<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <com.kimonic.utilsmodule.ui.MTopBarView
        android:id="@+id/mtb_act_finddatadetails"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="@color/colorQianLan"
        app:centerText="@string/xiugaijilu"
        app:leftImage="@drawable/xvector_back" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"

        >
        <TextView
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            style="@style/TextStyle001"
            android:text="@string/zichanleibie"
            />

        <TextView
            android:id="@+id/tv_lv_savedatabean_label"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            style="@style/TextStyle001"
            />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:paddingTop="5dp"
        android:paddingBottom="5dp" >
        <TextView
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            style="@style/TextStyle001"
            android:text="@string/zichanjiazhi"
            />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            style="@style/TextStyle001"
            android:id="@+id/tv_lv_savedatabean_shuzhi"
            />
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:paddingTop="5dp"
        android:paddingBottom="5dp" >
        <TextView
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            style="@style/TextStyle001"
            android:text="@string/jiluriqi"
            />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            style="@style/TextStyle001"
            android:id="@+id/tv_lv_savedatabean_record"
            />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:paddingTop="5dp"
        android:paddingBottom="5dp" >
        <TextView
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            style="@style/TextStyle001"
            android:text="@string/zichanbeizhu"
            />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            style="@style/TextStyle001"
            android:id="@+id/tv_lv_savedatabean_mark"
            />
    </LinearLayout>

</LinearLayout>