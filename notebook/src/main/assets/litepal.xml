<?xml version="1.0" encoding="utf-8"?>
<litepal>
    <!--数据库的名称-->
    <dbname value="notebook" />

    <!--数据库的版本号,当有改变时将版本号加1-->
    <version value="4" />

   <!--Java bean,对应相应的表名,必须写完整包路径-->
    <list>
        <!--用户名密码表-->
        <mapping class="com.kimonic.notebook.litemapbean.fixedassets.DataNameTableLMBean"/>
        <!--固定资产记录存储表-->
        <mapping class="com.kimonic.notebook.litemapbean.fixedassets.SaveDataLMBean"/>
        <!--固定资产资产类别记录表-->
        <mapping class="com.kimonic.notebook.litemapbean.fixedassets.ItemFlagLMBean"/>
        <!--固定资产记录日期存储表-->
        <mapping class="com.kimonic.notebook.litemapbean.fixedassets.DateRecordLMBean"/>
        <!--投资记录详情表-->
        <mapping class="com.kimonic.notebook.litemapbean.invest.InvestmentRecordDetailsLMBean"/>
        <mapping class="com.kimonic.notebook.litemapbean.invest.InvestDateLMBean"/>
        <mapping class="com.kimonic.notebook.litemapbean.invest.InvestPlateformLMBean"/>
        <mapping class="com.kimonic.notebook.litemapbean.invest.RepayDateLMBean"/>

    </list>
</litepal>